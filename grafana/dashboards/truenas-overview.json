        { "id": "joinByField", "options": { "byField": "task_name", "mode": "outer" } },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true,
              "Time 3": true,
              "Time 4": true,
              "Value #B": true,
              "Value #C": true,
              "__name__": true,
              "__name__ 1": true,
              "__name__ 2": true,
              "__name__ 3": true,
              "__name__ 4": true,
              "host 2": true,
              "host 3": true,
              "host 4": true,
              "instance": true,
              "instance 1": true,
              "instance 2": true,
              "instance 3": true,
              "instance 4": true,
              "job": true,
              "job 1": true,
              "job 2": true,
              "job 3": true,
              "job 4": true,
              "task_id": true,
              "task_id 1": true,
              "task_id 2": true,
              "task_id 3": true,
              "task_id 4": true
            },
            "indexByName": {
              "Value #A": 2,
              "Value #C": 4,
              "Value #D": 3,
              "host 1": 0,
              "task_name": 1
            },
            "renameByName": {
              "Value #A": "State",
              "Value #C": "Expected",
              "Value #D": "Last Run",
              "host 1": "Host",
              "task_name": "Task"
            }
